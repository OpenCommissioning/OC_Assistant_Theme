<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:theme="clr-namespace:OC.Assistant.Theme">
    
    <ControlTemplate x:Key="WarningIcon" TargetType="ContentControl">
        <ContentControl
            Template="{DynamicResource CombinedIconTemplate}"
            Background="{DynamicResource WarningBrush}"
            Foreground="{DynamicResource BackgroundBaseBrush}"
            Content="{DynamicResource IconStatusTriangleOuter}"
            theme:Properties.AdditionalContent="{DynamicResource IconStatusTriangleExclamation}"
            FontSize="{TemplateBinding FontSize}"/>
    </ControlTemplate> 
    
    <ControlTemplate x:Key="InformationIcon" TargetType="ContentControl">
        <Grid>
            <ContentControl 
                Template="{DynamicResource CombinedIconTemplate}"
                Background="{DynamicResource AccentBrush}"
                Foreground="{DynamicResource ForegroundBaseBrush}"
                Content="{DynamicResource IconStatusCircleOuter}"
                theme:Properties.AdditionalContent="{DynamicResource IconStatusCircleInfo}"
                FontSize="{TemplateBinding FontSize}"/>
        </Grid>
    </ControlTemplate>
    
    <ControlTemplate x:Key="ErrorIcon" TargetType="ContentControl">
        <Grid>
            <ContentControl 
                Template="{DynamicResource CombinedIconTemplate}"
                Background="{DynamicResource DangerBrush}"
                Foreground="{DynamicResource ForegroundBaseBrush}"
                Content="{DynamicResource IconStatusCircleOuter}"
                theme:Properties.AdditionalContent="{DynamicResource IconStatusCircleError}"
                FontSize="{TemplateBinding FontSize}"/>
        </Grid>
    </ControlTemplate>
    
    <ControlTemplate x:Key="QuestionIcon" TargetType="ContentControl">
        <Grid>
            <ContentControl 
                Template="{DynamicResource CombinedIconTemplate}"
                Background="{DynamicResource AccentBrush}"
                Foreground="{DynamicResource ForegroundBaseBrush}"
                Content="{DynamicResource IconStatusCircleOuter}"
                theme:Properties.AdditionalContent="{DynamicResource IconStatusCircleQuestionMark}"
                FontSize="{TemplateBinding FontSize}"/>
        </Grid>
    </ControlTemplate>
    
    <ControlTemplate x:Key="IconTemplate" TargetType="ContentControl">
        <Viewbox 
            Height="{TemplateBinding FontSize}"
            Width="{TemplateBinding FontSize}">
            <Path 
                Data="{TemplateBinding Content}" 
                Fill="{TemplateBinding Foreground}"/>
        </Viewbox>
    </ControlTemplate>
    
    <ControlTemplate x:Key="CombinedIconTemplate" TargetType="ContentControl">
        <Viewbox Height="{TemplateBinding FontSize}"
                 Width="{TemplateBinding FontSize}">
            <Grid>
                <Path 
                    Data="{TemplateBinding Content}"
                    Fill="{TemplateBinding Background}"/>
                <Path 
                    Data="{Binding Path=(theme:Properties.AdditionalContent), 
                        RelativeSource={RelativeSource TemplatedParent}}"
                    Fill="{TemplateBinding Foreground}"
                    Margin="{TemplateBinding Padding}"/>
            </Grid>
        </Viewbox>
    </ControlTemplate>
    
